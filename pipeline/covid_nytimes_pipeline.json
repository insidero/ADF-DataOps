					{
						"activity": "To Raw Partition",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "To Raw Overwrite",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
