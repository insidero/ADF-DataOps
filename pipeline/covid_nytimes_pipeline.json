			"rawBlobDirectory": {
				"type": "string",
				"defaultValue": "raw/covid-19/covid_tracking"
			},
			"curatedBlobDirectory": {
				"type": "string",
				"defaultValue": "curated/covid-19/covid_tracking"
			},
			"filename": {
				"type": "string",
				"defaultValue": "covid_tracking"
			}
		},
		"folder": {
			"name": "Case Data"
		},
		"annotations": [
			"covidtracking"
		]
